<launch>

<param name="use_sim_time" value="true" />

<!--<group ns="icab1" clear_params="true">-->
    <arg name="laser_topic" default="icab1/scan" />
    <arg name="output_topic" default="laser_to_gridmap" />
    <arg name="map_frame_id" default="icab1/base_footprint" />
    <arg name="height" default="150.0" />
    <arg name="width" default="100.0" />
    <arg name="cell_size" default="0.2" />
    <arg name="DEBUG" default="false" />


    <node pkg="hypergrid" type="laser_to_gridmap" name="laser_to_gridmap" output="screen">
        <param name="laser_topic" value="$(arg laser_topic)"/>
        <param name="output_topic" value="$(arg output_topic)"/>
        <param name="map_frame_id" value="$(arg map_frame_id)"/>
        <param name="height" value="$(arg height)"/>
        <param name="width" value="$(arg width)"/>
        <param name="cell_size" value="$(arg cell_size)"/>
        <param name="DEBUG" value="$(arg DEBUG)"/>
    </node>
    <node type="rviz" name="rviz" pkg="rviz" />
<!--</group>-->
</launch>
